generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int @id @default(autoincrement())
  createdAT DateTime @default(now())
  updatedAT DateTime @updatedAt

  name      String @unique
  password  String
  email     String @unique
  
  active    Boolean @default(true)

  @@map("users")
}